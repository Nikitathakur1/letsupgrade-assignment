#try 5 different function of the list object in python
list = [4,8,8,3,9,1]
list.sort()
print(list)

list.append(9)
print(list)

list.pop(3)
print(list)

count = list.count(8)
print(count)

name = ["nikki" , "neha" , "jay"]
print('original list:',name)
name.reverse()
print('updated list:', name)


